"xbindkeys_show" 
  control+shift + q


"amixer -q sset Master 2dB+"
    m:0x0 + c:123
    XF86AudioRaiseVolume
"amixer -q sset Master 2dB-"
    m:0x0 + c:122
    XF86AudioLowerVolume
"amixer -q sset Master toggle"
    m:0x0 + c:121
    XF86AudioMute

"lock_screen"
    m:0x0 + c:160
    XF86ScreenSaver

