Rxvt*background: black
Rxvt*foreground: white
Rxvt*font: xft:Terminus:size=12:lang=ru
Rxvt*scrollWithBuffer: False
Rxvt*scrollTtyOutput: False
Rxvt*scrollTtyKeypress: True
Rxvt*scrollBar: false
Rxvt*preeditType: OnTheSpot
Rxvt*inputMethod: uim
Rxvt*perl-ext: default,matcher,selection,selection-autotransform
Rxvt*selection-autotransform.0: s/^([^:[:space:]]+):(\\d+):?$/vim +$2 \\Q$1\\E\\x0d/
Rxvt*urlLauncher: firefox-bin

