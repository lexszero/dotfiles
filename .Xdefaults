Rxvt*background: black
Rxvt*foreground: white
Rxvt*font: xft:Terminus:pixelsize=12
Rxvt*scrollWithBuffer: False
Rxvt*scrollTtyOutput: False
Rxvt*scrollTtyKeypress: True
Rxvt*scrollBar: false
Rxvt*preeditType: OnTheSpot
Rxvt*inputMethod: uim
Rxvt*perl-ext: default,matcher,selection,selection-autotransform
Rxvt*selection-autotransform.0: s/^([^:[:space:]]+):(\\d+):?$/vim +$2 \\Q$1\\E\\x0d/
Rxvt*url-launcher: firefox
Rxvt*matcher.button: 3
Rxvt*urgentOnBell: true
URxvt.keysym.Mod4-0x30: command:\033]710;xft:Terminus:pixelsize=10\007\033]711;xft:Terminus:pixelsize=10:style=bold\007
URxvt.keysym.Mod4-0x2d: command:\033]710;xft:Terminus:pixelsize=13\007\033]711;xft:Terminus:pixelsize=13:style=bold\007
URxvt.keysym.Mod4-0x3d: command:\033]710;xft:Terminus:pixelsize=16\007\033]711;xft:Terminus:pixelsize=16:style=bold\007

Tkabber*font: DejaVuSans 12

Rxvt*color0:			black
Rxvt*color1:			#B00000
Rxvt*color2:			green4
Rxvt*color3:			yellow3
Rxvt*color4:			#4040FF
Rxvt*color5:			#A000A0
Rxvt*color6:			#009999
/* Rxvt*color7:			#D0B099 */
Rxvt*color8:			grey90
Rxvt*color9:			#FF3030
Rxvt*color10:			#70FF70
Rxvt*color11:			#F0F080
Rxvt*color12:			#B0B0FF
Rxvt*color13:			#EFA0BF
Rxvt*color14:			#A0FFFF
Rxvt*color15:			white

XTerm*background: black
XTerm*foreground: white





































